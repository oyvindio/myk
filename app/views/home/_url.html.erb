<div class="span-12 last url for-grey font-size-large air-15" id="<%=url.id%>">
       
              <div class="span-8" style="font-size:30px;">
                http://myk.is/<%=url.token%>
              </div>

              <div class="span-2 "style="font-size:30px;">
                <%=url.clicks%>
              </div>

              <div class="span-2 last">
                <div class="span-2">
                  <%=link_to "map", :action=>"show_map",:token=>url.token%>
                </div>
                <div class="span-3 pull-1 last">
                  <%=clippy("http://myk.is/#{url.token}") %>
                </div>
              </div>
             
              
              <div class="span-9 last">
                        From: twitter.com (<%=url.twitter_count%>), facebook.com (<%=url.facebook_count%>), others (<%=url.clicks-(url.twitter_count + url.facebook_count)%>)<br/>
              
              <%=url.url%></div>
        </div>
